<h2>{{encabezado}} </h2>

<nav>
  <div class="loguot">
    <form id="formLogout">
      <button type="submit">Logout</button>
    </form>
  </div>
  <div class="user">
    Usuario:{{nick}} <br>
    Roles: {{role}}
  </div>
</nav>

{{#if hayDocs}}
<ul>
    {{#each docs}}
  <div class="etiqueta">
    <li>id:{{this.id}}</li>
    <li>title: {{this.title}}</li>
    <li>description: {{this.description}}</li>
    <li>price: {{this.price}}</li>
    <li>thumbnail: {{this.thumbnail}}</li>
    <li>stock: {{this.stock}}</li>
    <li>code: {{this.code}}</li>
    <li>category: {{this.category}}</li>
    <li>status: {{this.status}}</li>
    <form id="formAddToCart">
      <button type="submit">Agregar al Carrito</button>
    </form>
  </div>
    {{/each}}
</ul>
{{else}}
array vacio...
{{/if}}

<a {{#if hasPrevPage}} href="/products/?limit={{limit}}&page={{prevPage}}" {{/if}}>Prev</a>
[{{ page }}]
<a {{#if hasNextPage}} href="/products/?limit={{limit}}&page={{nextPage}}" {{/if}}>Next</a><br>

<script src="/js/logout.js"></script>
<script src="/js/addToCart.js"></script>